{
  "$schema": "http://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs"
  },
  "exclude": [
    "./*\\.test\\.ts$"
  ],
  "jsc": {
    "externalHelpers": false,
    "paths": {
      "@": [
        "src/main"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    }
  }
}
